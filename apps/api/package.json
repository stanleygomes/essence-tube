{
  "name": "api",
  "type": "module",
  "version": "1.3.0",
  "scripts": {
    "start": "npx vercel dev",
    "start:fastify": "SERVER_PORT=5000 node --loader ts-node/esm src/infra/web/fastify/server.ts",
    "build": "echo 'No build step required'",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx vercel dev --listen 5000 --debug",
    "dev:fastify": "npm run start:fastify",
    "deploy": "npx vercel --prod",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --fix",
    "check-types": "tsc --noEmit",
    "containers:up": "docker-compose -f docker-compose.dev.yml up -d",
    "containers:down": "docker-compose -f docker-compose.dev.yml down"
  },
  "_moduleAliases": {
    "@": "src"
  },
  "dependencies": {
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@libsql/client": "^0.17.0",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.45.1",
    "express": "^5.1.0",
    "fastify": "^5.4.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.0",
    "mustache": "^4.2.0",
    "pino": "^9.7.0",
    "redis": "^5.6.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/axios": "^0.9.36",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongodb": "^4.0.6",
    "@types/mongoose": "^5.11.96",
    "@types/mustache": "^4.2.6",
    "@types/node": "^24.0.3",
    "@types/uuid": "^10.0.0",
    "@vercel/node": "^5.6.6",
    "eslint": "^9.39.1",
    "globals": "^16.2.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.1",
    "typescript": "5.9.2"
  }
}
